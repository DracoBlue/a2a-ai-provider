{
  "name": "a2a-ai-provider",
  "version": "0.1.0-alpha.13",
  "description": "Community A2A provider for the ai-sdk from vercel",
  "homepage": "https://github.com/dracoblue/a2a-ai-provider",
  "repository": {
    "type": "git",
    "url": "https://github.com/dracoblue/a2a-ai-provider.git",
    "directory": "."
  },
  "license": "MIT",
  "author": "DracoBlue <JanS@DracoBlue.de> (https://dracoblue.net)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm",
    "test": "vitest --run",
    "test:update": "vitest -u",
    "test:watch": "vitest -w",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "keywords": [
    "ai",
    "vercel-ai",
    "ai-sdk"
  ],
  "peerDependencies": {
    "ai": ">=5.0.0-beta.0 <6.0.0"
  },
  "devDependencies": {
    "@ai-sdk/provider": "2.0.0-beta.1",
    "@ai-sdk/provider-utils": "3.0.0-beta.2",
    "@ai-sdk/react": "2.0.0-beta.7",
    "@eslint/js": "^9.30.1",
    "@types/node": "^24.0.9",
    "@vitest/coverage-v8": "^3.2.4",
    "ai": "5.0.0-beta.3",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vitest": "^3.2.4",
    "zod": "^3.25.67"
  },
  "dependencies": {
    "@a2a-js/sdk": "^0.2.2"
  }
